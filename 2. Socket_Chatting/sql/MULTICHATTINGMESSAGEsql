CREATE TABLE MULTICHATMESSAGE(
    MULTICHATTINGMESSAGE_ID INT PRIMARY KEY,
    MULTICHATTINGROOM_ID INT,
    UNAME1 INT,
    CONTENT VARCHAR(4000),
    REGDATE DATE,
    
    CONSTRAINT FK_MULTICHATMESSAGE_ROOMID FOREIGN KEY (MULTICHATTINGROOM_ID) REFERENCES MULTICHATTINGROOM(MULTICHATTINGROOM_ID),
    CONSTRAINT FK_MULTICHATMESSAGE_UNAME1 FOREIGN KEY (UNAME1) REFERENCES MEMBER(ID)
);


CREATE SEQUENCE MULTICHATMESSAGE_ID START WITH 1 INCREMENT BY 1 MAXVALUE 999 CYCLE NOCACHE;